{
  "name": "atomic-date",
  "version": "1.0.0",
  "type": "module",
  "description": "Represent a date as a human-readable integer",
  "repository": "th3rius/atomic-date",
  "keywords": [
    "date"
  ],
  "author": "Gabriel de Oliveira <gabrielhk3@gmail.com> (https://nocnitsa.com)",
  "license": "MIT",
  "scripts": {
    "prepare": "husky",
    "build": "rimraf dist && pkgroll --sourcemap",
    "test": "node --test **/*.spec.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "pkgroll": "^2.24.0",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3"
  }
}
